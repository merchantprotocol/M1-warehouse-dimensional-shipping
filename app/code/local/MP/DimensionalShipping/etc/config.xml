<config>
    <modules>
        <MP_DimensionalShipping>
            <version>1.0.0.0</version>
        </MP_DimensionalShipping>
    </modules>
    <global>
        <blocks>
            <mp_dimensionalshipping>
                <class>MP_DimensionalShipping_Block</class>
            </mp_dimensionalshipping>
        </blocks>
        <helpers>
            <mp_dimensionalshipping>
                <class>MP_DimensionalShipping_Helper</class>
            </mp_dimensionalshipping>
            <wsacommon>
                <rewrite>
                    <template>MP_DimensionalShipping_Helper_Wsacommon_Template</template>
                </rewrite>
            </wsacommon>
            <wsafreightcommon>
                <rewrite>
                    <data>MP_DimensionalShipping_Helper_Wsafreightcommon_Data</data>
                </rewrite>
            </wsafreightcommon>
        </helpers>
    </global>
    <frontend>
        <layout>
            <updates>
                <wsafreightcommon>
                    <file>mp_dimensionalshipping.xml</file>
                </wsafreightcommon>
            </updates>
        </layout>
    </frontend>
</config>